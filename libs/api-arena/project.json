{
  "root": "libs/api-arena",
  "sourceRoot": "libs/api-arena/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/api-arena/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/api-arena"
      ],
      "options": {
        "jestConfig": "libs/api-arena/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}